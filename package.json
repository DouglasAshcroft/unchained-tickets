{
  "name": "unchained-tickets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run stop:all && npm run start:db && sleep 3 && npm run dev",
    "build": "next build",
    "start": "next start",
    "start:db": "docker start unchained-db || echo 'Database already running'",
    "stop:all": "pkill -f 'next' || true && pkill -f 'node.*server.js' || true",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:down:volumes": "docker compose down -v",
    "docker:logs": "docker compose logs -f postgres",
    "docker:reset": "docker compose down -v && docker compose up -d && sleep 5 && npx prisma migrate deploy && npx prisma db push --skip-generate && npm run db:seed",
    "docker:fresh": "docker stop unchained-db 2>/dev/null || true && docker rm unchained-db 2>/dev/null || true && npm run docker:reset"
  },
  "dependencies": {
    "@coinbase/cdp-sdk": "^1.38.4",
    "@coinbase/onchainkit": "^1.1.1",
    "@prisma/client": "^6.16.3",
    "@tanstack/react-query": "^5.81.5",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.1.0",
    "graphql": "^16.11.0",
    "graphql-request": "^6.1.0",
    "html5-qrcode": "^2.3.8",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.5.4",
    "qr": "^0.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "react-idle-timer": "^5.7.2",
    "react-qr-code": "^2.0.18",
    "sonner": "^2.0.7",
    "use-debounce": "^10.0.6",
    "viem": "^2.38.0",
    "wagmi": "^2.16.3",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.4",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "prisma": "^6.16.3",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
